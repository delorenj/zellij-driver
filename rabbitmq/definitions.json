{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "users": [
    {
      "name": "bloodbank",
      "password_hash": "",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "bloodbank",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "bloodbank-dev"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "event-store-ha",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "message-ttl": 86400000,
        "max-length": 100000
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "event_store_manager_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 100000,
        "x-overflow": "reject-publish",
        "x-dead-letter-exchange": "bloodbank.dlx",
        "x-dead-letter-routing-key": "event_store_manager_dlq"
      }
    },
    {
      "name": "event_store_manager_dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-message-ttl": 604800000
      }
    },
    {
      "name": "letta_handoff_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 10000
      }
    }
  ],
  "exchanges": [
    {
      "name": "bloodbank.events.v1",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "bloodbank.dlx",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "bloodbank.events.v1",
      "vhost": "/",
      "destination": "event_store_manager_queue",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
    },
    {
      "source": "bloodbank.events.v1",
      "vhost": "/",
      "destination": "letta_handoff_queue",
      "destination_type": "queue",
      "routing_key": "bmad.workflow.handoff.#",
      "arguments": {}
    },
    {
      "source": "bloodbank.dlx",
      "vhost": "/",
      "destination": "event_store_manager_dlq",
      "destination_type": "queue",
      "routing_key": "event_store_manager_dlq",
      "arguments": {}
    }
  ]
}
